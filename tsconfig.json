{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
     "baseUrl": "./",
     "paths": {
       "src/*": [
         "./src/*"
       ]
     },
     "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,

    "strict": true,
    "strictNullChecks": true,
    "skipLibCheck": true
  }
}
